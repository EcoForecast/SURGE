<script type="text/javascript">
	function buildLinks(){
	var params = "";
	var TWCname = $("input[name='TWCcityname']").val();
	if(TWCname){params="locid="+TWCname;}
	else{
		params+="req_city="+$('input[name=search_city]').val();
		params+="&req_state="+$('input[name=search_state]').val();
		params+="&req_statename="+$('input[name=search_statename]').val();
  }
  params += "&reqdb.zip=" + $('input[name=search_zip]').val();
  params += "&reqdb.magic=" + $('input[name=search_magic]').val();
  params += "&reqdb.wmo=" + $('input[name=search_wmo]').val();
  var prevVal = $('.previous-link a').attr('href');
  var nextVal = $('.next-link a').attr('href');
  $('.previous-link a').attr('href', prevVal+"?"+params);
  $('.next-link a').attr('href', nextVal+"?"+params);
  }
	function buildPWSUrl() {
  var new_date = new Date();
  new_date = new_date - 24 * 60 * 60 * 1000;
  var yesterday = new Date(new_date);
  var yesterday_date = wui.date.strftime("%Y%m%d", yesterday);
  new_date = new_date - 24 * 60 * 60 * 30 * 1000;
  var month_ago = new Date(new_date);
  var thirty_days_ago = wui.date.strftime("%Y%m%d", month_ago);
  //if(wui.bootstrapped.pwsdashboard.crossLinkingVars.histType=="yesterday"){
  yesterday_location = '/personal-weather-station/dashboard?ID=' + $('input[name=local_station]').val() + '&scrollTo=historyTable#history/s' + thirty_days_ago + '/e' + yesterday_date + '/mmonth';
  $('a#pws_station').attr('href', yesterday_location);
}
buildLinks();
buildPWSUrl();
</script>
