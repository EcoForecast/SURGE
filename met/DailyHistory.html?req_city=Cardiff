	<script>
		(function($) {
		var wfx_url = '//triggers1.wfxtriggers.com/json/?resp_type=json&acctid=' + wfx_id + '&current=true&df=1,2,3&zcs=&nzcs=00000';
		$.ajax({
		url: wfx_url,
		dataType: 'jsonp',
		cache: true,
		type: 'GET',
		timeout: 5000,
		success: function(data) {
		if (data && data.wfxtg) {
		var types = ['zcs', 'hzcs', 'nzcs'];
		if (data.wfxtg.current) {
		utag_data.weather_fx = data.wfxtg.current.join(',');
		}
		if (data.wfxtg.scatterSegs) {
		for (var i = 0, len = data.wfxtg.scatterSegs.length; i < len; i++) {
								var seg = data.wfxtg.scatterSegs[i];
								var type;
								for (var key in seg) {
									if (seg.hasOwnProperty(key)) {
						                type = key;
						            }
								}

								if (type.length && seg[type].length && $.inArray(type, types) > -1) {
		var segments = seg[type][0].segments;
		if (segments && segments.length) {
		utag_data['weather_fx_' + type] = segments.join(',');
		}
		}
		}
		}
		}
		},
		error: function(){
		console.log('wfxtriggers failed');
		}
		});
		var url2 = '//ad.crwdcntrl.net/5/c=2215/pe=y/callback=?';
		$.ajax({
		url: url2,
		dataType: 'jsonp',
		cache: true,
		type: 'GET',
		timeout: 5000,
		success: function(data) {
		utag_data['lotame_audience'] = '';
		if(data && data.Profile && data.Profile.Audiences && data.Profile.Audiences.Audience) {
		for(var i = 0; i < data.Profile.Audiences.Audience.length; i++) {
							if(i > 0) {
		utag_data['lotame_audience'] = utag_data['lotame_audience'] + ',';
		}
		utag_data['lotame_audience'] = utag_data['lotame_audience'] + data.Profile.Audiences.Audience[i].id;
		}
		}
		utag_data['tpid'] = data && data.Profile && data.Profile.tpid || '';
		if (window._cc2216) {
		if(utag_data.dma) { _cc2216.add("seg", "dma_" + utag_data.dma); }
		if(utag_data.state) { _cc2216.add("seg", "state_" + utag_data.state); }
		if(utag_data.country_code) { _cc2216.add("seg", "country_" + utag_data.country_code); }
		if(utag_data.language) { _cc2216.add("seg", "lang_" + utag_data.language); }
		if(utag_data.temperature) { _cc2216.add("seg", "temp_" + utag_data.temperature); }
		if(utag_data.temperature_range) { _cc2216.add("seg", "tempR_" + utag_data.temperature_range); }
		if(utag_data.pollen) { _cc2216.add("seg", "pollen_" + utag_data.pollen); }
		if(utag_data.humidity) { _cc2216.add("seg", "hum_" + utag_data.humidity); }
		if(utag_data.condition) { _cc2216.add("seg", "cond_" + utag_data.condition); }
		if(utag_data.forecast_high_imcrement) { _cc2216.add("seg", "tempH_" + utag_data.forecast_high_imcrement); }
		if(utag_data.forecast_high_range) { _cc2216.add("seg", "tempHR_" + utag_data.forecast_high_range); }
		if(utag_data.forecast_low_increment) { _cc2216.add("seg", "tempL_" + utag_data.forecast_low_increment); }
		if(utag_data.forecast_low_range) { _cc2216.add("seg", "tempLR_" + utag_data.forecast_low_range); }
		if(utag_data.forecast_day_1) { _cc2216.add("seg", "fc1_" + utag_data.forecast_day_1); }
		if(utag_data.forecast_day_2) { _cc2216.add("seg", "fc2_" + utag_data.forecast_day_2); }
		if(utag_data.forecast_day_3) { _cc2216.add("seg", "fc3_" + utag_data.forecast_day_3); }
		if(utag_data.barometer) { _cc2216.add("seg", "baroTendency_" + utag_data.barometer); }
		_cc2216.bcp();
		}
		},
		error: function(){
		console.log('lotame adcontrol failed');
		}
		});
		})(jQuery);
